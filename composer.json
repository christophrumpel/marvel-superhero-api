{
    "name": "christophrumpel/marvel-superhero-api",
    "description": "This is a PHP package for working with the Marvel developer API.",
    "keywords": [
        "christophrumpel",
        "marvel-superheeor-api"
    ],
    "homepage": "https://github.com/christophrumpel/marvel-superhero-api",
    "license": "MIT",
    "authors": [
        {
            "name": "Christoph Rumpel",
            "homepage": "http://christoph-rumpel.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php" : ">=5.3.0",
        "guzzlehttp/guzzle": "~6.2"
    },
    "require-dev": {
        "phpunit/phpunit" : "4.*"
    },
    "autoload": {
        "psr-4": {
            "Rumpel\\MarvelUniverse\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Rumpel\\MarvelUniverse\\Test\\": "tests"
        }
    }
}
